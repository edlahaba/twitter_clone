<!DOCTYPE html>
<html>
  <head>
    <%= render 'layouts/rails_default' %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
   <%= render 'layouts/header' %>
   <div class='container'>
    <% flash.each do |key, message| %>
      <%= content_tag(:div, message, class: ["alert", "alert-#{key}"]) %>
    <% end %>
    <%= yield %>
    <%= render 'layouts/footer' %>
    <%= debug(params) if Rails.env.development? %>
   </div>
  </body>
</html>
